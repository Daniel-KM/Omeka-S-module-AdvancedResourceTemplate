{
    "name": "daniel-km/omeka-s-module-advanced-resource-template",
    "type": "omeka-s-module",
    "description": "Module for Omeka S: Add new features to the resource templates to simplify and to improve the edition of resources: closed template, auto-completion, property groups, select language, lock, auto-filling, etc.",
    "keywords": [
        "Omeka S",
        "Omeka S module",
        "admin",
        "resource edition",
        "curation"
    ],
    "license": "CECILL-2.1",
    "homepage": "https://gitlab.com/Daniel-KM/Omeka-S-module-AdvancedResourceTemplate",
    "authors": [
        {
            "name": "Daniel Berthereau",
            "email": "Daniel.packagist@Berthereau.net"
        }
    ],
    "support": {
        "issues": "https://gitlab.com/Daniel-KM/Omeka-S-module-AdvancedResourceTemplate/-/issues",
        "forum": "https://forum.omeka.org/"
    },
    "_require (not set until Omeka S merges PR #2412)": {
        "omeka/omeka-s": "^4.0",
        "daniel-km/omeka-s-module-common": "*"
    },
    "autoload": {
        "psr-4": {
            "AdvancedResourceTemplate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdvancedResourceTemplateTest\\": "tests/AdvancedResourceTemplateTest/"
        }
    },
    "extra": {
        "installer-name": "AdvancedResourceTemplate"
    }
}
